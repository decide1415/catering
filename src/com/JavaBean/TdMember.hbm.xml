<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.JavaBean.TdMember" table="td_member" schema="catering">
        <composite-id mapped="true" class="com.JavaBean.TdMemberPK">
            <key-property name="id" column="id"/>
            <key-property name="py" column="py"/>
        </composite-id>
        <property name="name" column="name"/>
        <property name="pyfist" column="pyfist"/>
    </class>
</hibernate-mapping>