        <property name="time" column="time"/>
        <property name="cotTcAbySta" column="cotTCAbySTA"/>
        <property name="cotTcBbySta" column="cotTCBbySTA"/>
        <property name="cotTcAbyStb" column="cotTCAbySTB"/>
        <property name="cotTcBbyStb" column="cotTCBbySTB"/>
        <property name="userName" column="user_name"/>
